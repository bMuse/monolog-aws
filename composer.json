{
    "name": "bmuse/monolog-aws",
    "description": "Send logs using aws api",
    "keywords": ["log", "aws", "logging", "kinesis", "cloudwatch"],
    "homepage": "http://github.com/bMuse/monolog-aws",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Daniel Guerrero",
            "email": "daniel@bmuse.com",
            "homepage": "http://opensource.bmuse.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": ">= 1.0.0",
        "symfony/console": ">= 2.5.0",
        "aws/aws-sdk-php": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "jakub-onderka/php-parallel-lint": "^0.9"
    },
    "autoload": {
        "psr-4": {"Bmuse\\Monolog\\Aws\\": "src/Bmuse/Monolog/Aws"}
    },
    "autoload-dev": {
       "psr-4": {"Bmuse\\Monolog\\Aws\\Tests\\": "tests/Bmuse/Monolog/Aws/Tests"}
   },
   "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit"
        ]
    }
}
